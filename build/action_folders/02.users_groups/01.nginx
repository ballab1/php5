
declare -A nginx=()

declare ngx_uid=${nginx_uid:-1001}
declare ngx_gid=${nginx_gid:-1001}

nginx['user']=${nginx_user:-'nginx'}
nginx['uid']=${ngx_uid:-$(getent passwd "${nginx['user']}" | cut -d: -f3)}
nginx['group']=${nginx_group:-'nginx'}
nginx['gid']=${ngx_gid:-$(getent group "${nginx['user']}" | cut -d: -f3)}
nginx['shell']=/sbin/nologin
